var metric1 = stream
	|from()
		.database('sampledb')
		.retentionPolicy('default')
		.measurement('sample')
		.groupBy('node')
	|window()
		.align()
		.period(5m)
		.every(5m)
	|shift(-5m)
	|mean('metric1').as('metric1')


