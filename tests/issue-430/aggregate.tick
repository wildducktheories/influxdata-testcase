var aggregate = stream
	|from()
		.database('sampledb')
		.retentionPolicy('default')
		.measurement('sample')
		.where(lambda: "node" == 'C578A231-DB09-457B-B5FD-C86ABD135413')
		.groupBy('node')
	|window()
		.align()
		.period(5m)
		.every(5m)
	|mean('metric1').as('metric1')
	|influxDBOut()
		.database('sampledb')
		.measurement('actual')
