var metric1 = stream
	|from()
		.database('sampledb')
		.retentionPolicy('default')
		.measurement('sample')
	|window()
		.align()
		.period(5m)
		.every(5m)
	|groupBy('node')
	|mean('metric1').as('metric1')


var metric2 = stream
	|from()
		.database('sampledb')
		.retentionPolicy('default')
		.measurement('sample')
	|window()
		.align()
		.period(5m)
		.every(5m)
	|groupBy('node')
	|mean('metric2').as('metric2')

metric1
	|join(metric2)
	|log()
